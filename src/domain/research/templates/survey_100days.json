      "allowOther": true
    },
    {
      "id": "q4",
      "type": "text",
      "prompt": "Where do you see recurring risk vs. opportunity?"
    },
    {
      "id": "q5",
      "type": "likert",
      "prompt": "Would live visualization change your confidence in decisions?",
